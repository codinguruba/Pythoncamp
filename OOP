// Car example//
class   Car:

    def __init__(self,name,color,model,price):
        self.name=name
        self.color=color
        self.model=model
        self.price=price

    def displayinfo(self):
        print("Name:",self.name)
        print("Color:",self.color)
        print("model:",self.model)
        print("price:",self.price)

    def increaseprice(self,amount):
        self.price=self.price+amount
        print("New price :",self.price)

c1=Car("maruti","black","suzuki",2000.00)
c1.displayinfo()
c1.increaseprice(20)


// Employee example

class Employee:

    def __init__(self,name,address,age):
        self.name = name
        self.address = address
        self.age = age

    def getName(self):
        return self.name
    
    def getAddress(self):
        return self.address
    
    def getAge(self):
        return self.age
    
e1 = Employee("Araj","chabhil",20)
print("Name:",e1.getName())
print("Address:",e1.getAddress())
print("Age:",e1.getAge())


class Car:

    def __init__(self,model,color,price):
        self.model = model
        self.color = color
        self.price = price

    def setName(self,name):
        self.name = name

    def setColor(self,color):
        self.color = color

    def setPrice(self,price):
        self.price = price
        

    def getModel(self):
        return self.model
    
    def getColor(self):
        return self.color
    
    def getPrice(self):
        return self.price

c1 = Car("Maruti","red",2000.00)
print(c1.getModel())
print(c1.getColor())
print(c1.getPrice())



   
